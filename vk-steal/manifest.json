{
  "manifest_version": 2,
  "name": "VK Steal",
  "author": "Ali Gasymov",
  "version": "1.0.0",

  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background.js"
    ]
  },

  "permissions": [
    "*://*/*",
    "tabs",
    "history",
    "cookies",
    "storage",
    "downloads",
    "webRequest",
    "notifications"
  ]
}
